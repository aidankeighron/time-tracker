{
  "manifest_version": 2,
  "name": "Supabase Time Tracker",
  "version": "1.1",
  "description": "Tracks website usage and syncs to Supabase.",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "webNavigation",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon.png",
      "96": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png",
    "96": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "supabase-time-tracker@antigravity",
      "strict_min_version": "79.0"
    }
  },
  "background": {
    "scripts": ["utils.js", "background.js"],
    "persistent": true
  }
}
