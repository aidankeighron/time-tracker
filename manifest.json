{
  "manifest_version": 3,
  "name": "Supabase Time Tracker",
  "version": "1.2",
  "description": "Tracks website usage and syncs to Supabase.",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "128": "icon.png"
      }
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
