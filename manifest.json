{
  "manifest_version": 3,
  "name": "Supabase Time Tracker",
  "version": "1.0",
  "description": "Tracks website usage and syncs to Supabase.",
  "permissions": [
    "tabs",
    "storage",
    "alarms",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
